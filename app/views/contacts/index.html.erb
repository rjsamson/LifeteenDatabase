<div class="navbar">
<form class="navbar-search pull-left">
  <input id="contact-search" name="s" type="text" class="search-query" autocomplete="off" placeholder="Search Contacts">
</form>
<div class="btn-group">
  <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
    Action
    <span class="caret"></span>
  </a>
  <ul class="dropdown-menu">
    <!-- dropdown menu links -->
  </ul>
</div>
</div>
<h2>Contacts</h2>
<div class="btn-group">
  <button class="btn" onclick="window.location.href='<%=new_contact_path%>'">
    <%= link_to 'New Contact', new_contact_path %>
  </button>
  <ul class="thumbnails">
  <% if @contacts then @contacts.each do |contact| %>
    <li class="span4">
      <div class="thumbnail">
        <%=link_to raw("<img data-src='holder.js/160x120' alt='"+contact.first_name + " " + contact.last_name + "' src='"+contact.pic_url+"' width='120'>"), edit_contact_path(contact)%>
      </div>
        <h4><%=link_to contact.first_name + " " + contact.last_name, edit_contact_path(contact)%></h4>
    </li>
  <% end end %>
  </ul>
</div>


<script type="text/javascript">

$.getJSON('/contacts.json', function(data) {
  var contacts = [];
 
  $.each(data, function(key, val) {
    contacts.push(val.first_name + " " + val.last_name);
  });
$("#contact-search").typeahead({
    source : contacts
});
});

var colors = ["red", "blue", "green", "yellow", "brown", "black"];
 
$('#search').typeahead({source: colors});
</script>



